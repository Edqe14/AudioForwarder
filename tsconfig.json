{
  "compilerOptions": {
    "moduleResolution": "node",
    "declaration": false,
    "removeComments": false,
    "pretty": false,
    "outDir": "dist",
    "esModuleInterop": true,
    "module": "commonjs",
    "target": "esnext",
    "lib": [
      "esnext",
      "esnext.array",
      "esnext.asynciterable",
      "esnext.intl",
      "esnext.symbol"
    ],
    "noImplicitAny": true,
    "sourceMap": false,
    "skipDefaultLibCheck": true,
    "baseUrl": "./src/",
    "paths": {
      "@utils/*": ["utils/*"]
    }
  }
}